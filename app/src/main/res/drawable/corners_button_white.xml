    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffd6d6d6" android:endColor="#fffdfdfd" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ffb6b6b6" />
            <corners android:topLeftRadius="10.0dip" android:topRightRadius="10.0dip" android:bottomLeftRadius="10.0dip" android:bottomRightRadius="10.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#fffdfdfd" android:endColor="#ffd6d6d6" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ffb6b6b6" />
